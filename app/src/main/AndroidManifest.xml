<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.praba.ipfire">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Construction Us"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AppMainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"
            android:parentActivityName=".AppMainActivity"/>
        <activity android:name=".HomeActivity" />
        <activity
            android:name=".MyProject"
            android:parentActivityName=".HomeActivity" />
        <activity
            android:name=".NewProject"
            android:parentActivityName=".HomeActivity" />
        <activity android:name=".SettingsActivity" />
        <activity android:name=".MyProfile" />
        <activity
            android:name=".ChatActivity"
            android:parentActivityName=".HomeActivity" />
        <activity android:name=".ProjectDetailsActivity" />
        <activity
            android:name=".MyAccountActivity"
            android:parentActivityName=".HomeActivity" />
        <activity android:name=".LoginActivity"
            android:parentActivityName=".AppMainActivity"></activity>
    </application>

</manifest>